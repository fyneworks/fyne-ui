    "build:prod": "NODE_ENV=production webpack --config webpack.config.js --mode=production --env.NODE_ENV=production",
    "build": "npm run build:prod",
    "prebuild": "rm -rf ./dist && rm -rf ./demo",
    "ignore-deploy:surge": "surge ./demo --domain https://ui.fyneworks.io",
